{
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": ["plugin:@typescript-eslint/recommended", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "env": {
    "es2021": true,
    "browser": true
  },
  "rules": {
    "react/react-in-jsx-scope": 0
  }
}
